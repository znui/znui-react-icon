{
  "name": "znui-react-icon",
  "version": "1.0.6",
  "description": "Web icon component.",
  "main": "development.js",
  "scripts": {
    "webpack-example": "rimraf example/dist && webpack --config webpack.config.example.js",
    "webpack-example-watch": "rimraf example/dist && webpack --config webpack.config.example.js --watch",
    "clean-dist": "rimraf dist",
    "clean-build": "rimraf build",
    "webpack": "rimraf dist && webpack",
    "webpack-watch": "rimraf dist && webpack --watch",
    "build": "npm run clean-build && babel src -d build",
    "dist": "npm run clean-dist && webpack --uglify",
    "release": "npm run build && npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/znui/znui-react-icon.git"
  },
  "keywords": [
    "web component",
    "icon"
  ],
  "author": "yangyxu",
  "email": "xuyy0618@qq.com",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "znui-react": "^1.3.5"
  }
}
